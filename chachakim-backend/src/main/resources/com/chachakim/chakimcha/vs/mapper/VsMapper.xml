<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chachakim.chakimcha.vs.mapper.VsMapper">
      
  <select id = "view" resultType = "com.chachakim.chakimcha.vs.vo.VsVO">
    select vs_id, title, write_datetime, content, view_count
    from vs
    where vs_id in (#{vs_id})
  </select>

</mapper>
