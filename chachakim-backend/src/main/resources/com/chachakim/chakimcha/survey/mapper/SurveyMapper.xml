<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chachakim.chakimcha.survey.mapper.SurveyMapper">
      
  <select id = "view" resultType = "com.chachakim.chakimcha.survey.vo.SurveyVO">
    select survey_id, title, write_datetime, content, view_count
    from survey
    where survey_id in (#{survey_id})
  </select>

</mapper>
