<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chachakim.chakimcha.user.mapper.UserMapper">
      
  <select id = "view" resultType = "com.chachakim.chakimcha.user.vo.UserVO">
    select user_id, title, write_datetime, content, view_count
    from user
    where user_id in (#{user_id})
  </select>

</mapper>
